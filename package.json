{
  "name": "auth0-lock-passwordless",
  "version": "1.0.1",
  "description": "Auth0 Lock Passwordless",
  "author": "Auth0 <support@auth0.com> (http://auth0.com)",
  "license": "MIT",
  "keywords": [
    "auth0",
    "auth",
    "openid",
    "authentication",
    "passwordless",
    "browser",
    "jwt"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/auth0/lock-passwordless"
  },
  "main": "src/index.js",
  "browserify": {
    "transform": [
      [
        "browserify-css",
        {
          "minify": true
        }
      ],
      [
        "babelify",
        {
          "blacklist": [
            "regenerator"
          ],
          "optional": [
            "es7.functionBind",
            "es7.objectRestSpread"
          ]
        }
      ],
      [
        "packageify"
      ]
    ]
  },
  "scripts": {
    "start": "grunt dev",
    "build": "grunt build",
    "design": "grunt design",
    "test": "zuul -- test/*/*.test.js",
    "test:browser": "zuul --local 8080 --disable-tunnel -- test/*/*.test.js",
    "test:phantom": "mochify --recursive --extension=.jsx"
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "browserify-css": "^0.6.1",
    "bump-version": "^0.5.0",
    "expect.js": "^0.3.1",
    "grunt": "^0.4.5",
    "grunt-aws-s3": "^0.14.0",
    "grunt-browserify": "^4.0.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-connect": "^0.11.2",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-stylus": "^0.22.0",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-env": "^0.4.4",
    "grunt-http": "^1.6.0",
    "mochify": "^2.12.0",
    "packageify": "^0.2.2",
    "sinon": "^1.15.4",
    "unreleased": "^0.1.0",
    "zuul": "^3.2.0",
    "zuul-ngrok": "3.0.0"
  },
  "dependencies": {
    "auth0-js": "^6.7.2",
    "blueimp-md5": "^1.1.0",
    "fbjs": "^0.3.1",
    "immutable": "^3.7.3",
    "jsonp": "^0.2.0",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "trim": "0.0.1"
  }
}
